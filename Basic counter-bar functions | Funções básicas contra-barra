EN:
#include <stdio.h>
void main(){
    printf("=-=-=-=-=-=-=-=-=-=-=-=-=-=-=\n  \\a \t=\t beep\n");
    printf("  \\t \t=\t tabulation\n");
    printf("  \\n \t=\t line jump\n");
    printf("  %%%% \t=\t percentage\n");
    printf("  \\\\ \t=\t counterbar\n");
    printf("  \\? \t=\t interrogation\n");
    printf("=-=-=-=-=-=-=-=-=-=-=-=-=-=-=");
}
