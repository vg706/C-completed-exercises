PT/BR:
#include <stdio.h>
#include <locale.h>
void main(){
    setlocale(LC_ALL, "Portuguese");
    printf("=-=-=-=-=-=-=-=-=-=-=-=-=-=-=\n  \\a \t=\t beep\n");
    printf("  \\t \t=\t tabulação\n");
    printf("  \\n \t=\t pular linha\n");
    printf("  %%%% \t=\t porcentagem\n");
    printf("  \\\\ \t=\t contrabarra\n");
    printf("  \\? \t=\t interrogação\n");
    printf("=-=-=-=-=-=-=-=-=-=-=-=-=-=-=");
}
