#include <stdio.h>
main(){
    printf("=-=-=-=-=-=-=-=-=-=-=-= \n\\a \t= \tbeep");
    printf(" \n\\t \t= \ttab");
    printf(" \n\\n \t= \tline jump");
    printf(" \n\\ \t= \tcounter-bar");
    printf(" \n%% \t= \tpercentage");
}
