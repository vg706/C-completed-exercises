PT/BR:
#include <stdio.h>
#include <locale.h>
main(){
    setlocale(LC_ALL, "Portuguese");
    printf("=-=-=-=-=-=-=-=-=-=-=-= \n\\a \t= \tbeep");
    printf(" \n\\t \t= \ttabulação");
    printf(" \n\\n \t= \tpular linha");
    printf(" \n\\ \t= \tcontra barra");
    printf(" \n%% \t= \tporcentagem");
}
